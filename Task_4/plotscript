set terminal png size 800,800
set xlabel "Количество ядер"
set xrange [32:512]

set output 'plot1.png'
set ylabel "Время"
plot "report" using 1:3 w l lw 2 lc rgbcolor "#0000ff" title "Среднее время" , \
"report" using 1:4 w l lw 2 lc rgbcolor "#ff0000" title "Максимальное время"

set output 'plot2.png'
set ylabel "Ускорение"
set nokey
plot "report" using 1:2 w l lw 2 lc rgbcolor "#0000ff"
